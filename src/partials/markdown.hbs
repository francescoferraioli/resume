{{#each markdown}}
{{#switch this.type}} 
{{#case 'markdown'}}
{{#content ../class}}{{{this.markdown}}}{{/content}}
{{/case}} 
{{#case 'html'}}
{{#content ../class}}{{{this.html}}}{{/content}}
{{/case}} 
{{#case 'line-break'}}
{{> partial partial="spacer" height=this.height }}
{{/case}} 
{{/switch}}
{{/each}}